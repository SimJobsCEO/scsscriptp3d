<Gauge Name="Inter Left" Version="1.0">
   <Element>
      <Select>
         <Value>(A:Spoilers left position, percent) 10 &gt; (L:AnnCheck,bool) || </Value>
         <Case Value="0">
            <Image Name="lamp_red_off.bmp" ImageSizes="40,40,0,0"/>
         </Case>
         <Case Value="1">
            <Image Name="lamp_red_on.bmp" Bright="Yes" ImageSizes="40,40,0,0"/>
         </Case>
      </Select>
   </Element>
   <Mouse>
      <Help ID="HELPID_GAUGE_SPOILER"/>
   </Mouse>
</Gauge>
