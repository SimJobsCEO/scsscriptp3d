         <Value>(A:GENERAL ENG1 FUEL PUMP ON,bool)</Value>
         <Case Value="0">
            <Image Name="sw_d.bmp" ImageSizes="36,103,0,0"/>
         </Case>
         <Case Value="1">
            <Image Name="sw_u.bmp" ImageSizes="36,103,0,0"/>
         </Case>
      </Select>
   </Element>

   <Mouse>
      <Cursor Type="Hand"/>
      <Click>1 (>L:APU Fail,bool) 0 (>L:APU Ready,bool) 0 (>K:TOGGLE_ELECT_FUEL_PUMP1) 1 (>L:TumblerPressed)</Click>
   </Mouse>
</Gauge>
