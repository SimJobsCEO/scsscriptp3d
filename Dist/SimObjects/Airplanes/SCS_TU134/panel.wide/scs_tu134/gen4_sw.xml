<Gauge Name="Gen4 switch" Version="1.0">
   <Element>
      <Select>
         <Value>(L:Eng4_gen,bool)</Value>
         <Case Value="0">
            <Image Name="sw_d.bmp" ImageSizes="36,103,0,0"/>
         </Case>
         <Case Value="1">
            <Image Name="sw_u.bmp" ImageSizes="36,103,0,0"/>
         </Case>
      </Select>
   </Element>
   <Mouse>
      <Cursor Type="Hand"/>
      <Click>(L:Eng4_gen,bool) ! (>L:Eng4_gen,bool) 1 (>L:TumblerPressed)</Click>
   </Mouse>
</Gauge>
