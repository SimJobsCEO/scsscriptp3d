<Gauge Name="Hydr1 Fail" Version="1.0">
   <Element>
      <Select>
         <Value>(A:Circuit general panel on, bool) if{ (A:Hydraulic1 pressure, psi) (A:Hydraulic2 pressure, psi) + 5000 &lt; }</Value>
         <Case Value="0">
            <Image Name="lamp_red_off.bmp" ImageSizes="40,40,0,0"/>
         </Case>
         <Case Value="1">
            <Image Name="lamp_red_on.bmp" Bright="Yes" ImageSizes="40,40,0,0"/>
         </Case>
      </Select>
   </Element>
</Gauge>
